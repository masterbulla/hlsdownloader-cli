{
  "name": "hlsdownloader-cli",
  "version": "1.0.0-alpha.01",
  "description": "Downloads HLS Playlist file and TS chunks with Terminal",
  "preferGlobal": true,
  "bin": {
    "hlsdl": "bin/cli.js"
  },
  "scripts": {
    "compile": "babel index.js -o bin/cli.js --minified --no-comments",
    "lint": "eslint . tests/*.spec.js && echo 'No linting error found'",
    "lint:fix": "eslint . --fix",
    "test": "mocha --require @babel/register tests/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmrony/hlsdownloader-cli.git"
  },
  "bugs": {
    "url": "https://github.com/nmrony/hlsdownloader-cli/issues",
    "email": "pro.nmrony@gmail.com"
  },
  "author": "Nur Rony <pro.nmrony@gmail.com>(http://nmrony.info)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.32",
    "@babel/core": "^7.0.0-beta.32",
    "@babel/preset-env": "^7.0.0-beta.32",
    "@babel/register": "^7.0.0-beta.34",
    "@types/node": "^8.0.54",
    "babel-eslint": "^8.0.3",
    "chai": "^4.1.2",
    "eslint": "^4.12.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1"
  },
  "dependencies": {
    "minimist": "^1.2.0"
  }
}
